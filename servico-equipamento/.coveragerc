[run]
source = .
omit =
    */tests/*
    */conftest.py
    */__pycache__/*
    */venv/*
    */virtualenv/*
    exemplo_banco.py
    test_*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2
show_missing = True

[html]
directory = htmlcov
